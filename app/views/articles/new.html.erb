<div class="banner">
    <h1><%= link_to 'Wikinimous', articles_path %></h1>
    <p><%= link_to 'New Artcle', new_article_path %></p>
  </div>

<h1>New Article</h1>
<div class="form">
  <%= form_for(@article) do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title, class: "input"%>

  <%= f.label :content %>
  <%= f.text_area :content, class: "input" %>

  <%= f.submit %>
<% end %>
</div>

<%= link_to "Back to list", articles_path %>
